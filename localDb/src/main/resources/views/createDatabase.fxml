<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.fedorenko.db.controllers.DatabaseCreateController"
            prefHeight="95.0" prefWidth="300.0">
    <AnchorPane layoutX="-1.0" prefHeight="110.0" prefWidth="300.0">
        <TextField fx:id="textField" layoutX="15.0" layoutY="35.0" prefHeight="25.0" prefWidth="280.0"/>
        <HBox alignment="CENTER" layoutX="40.0" layoutY="75.0" spacing="30.0">
            <Button defaultButton="true" layoutX="45.0" layoutY="70.0" mnemonicParsing="false"
                    prefHeight="25.0" prefWidth="100.0" text="OK" onMouseClicked="#submit"/>
            <Button cancelButton="true" layoutX="175.0" layoutY="70.0" mnemonicParsing="false"
                    prefHeight="25.0" prefWidth="100.0" text="Cancel" onMouseClicked="#close"/>
        </HBox>
        <Label layoutX="15.0" layoutY="15.0" prefHeight="15.0" prefWidth="160.0" text="Enter database name:"/>
    </AnchorPane>
</AnchorPane>
